page {
  background: #fff;
}
.common-header {
  background: transparent;
}

.Bg {
  width: 100%;
  height: 406rpx;
  background: url(https://vip-account.oss-cn-hangzhou.aliyuncs.com/mobile/static/account/urs/image/income.jpg) no-repeat center center;
  background-size: contain;
  margin-top: -1rpx;
  position: relative;
}

.userData {
  width: 100%;
  height: 120rpx;
  position: absolute;
  top: 124rpx;
  text-align: center;
}

.userData > view {
  width: 33.33%;
  float: left;
}

.userData > view:nth-child(1){
  color: #fff;
}

.userData > view:nth-child(2){
  color: #47a4ff;
}

.userData > view:nth-child(3){
  color: #e9bc38;
}

.userData > view view:nth-child(1){
  font-size: 42rpx;
  overflow: hidden;
  text-overflow: ellipsis;
}

.userData > view view:nth-child(2){
  font-size: 22rpx;
}

.weekOrTotal {
  width: 100%;
  height: 70rpx;
  border-top: 18rpx solid #eee;
  position: absolute;
  left: 0;
  top: 300rpx;
}

.weekOrTotal > view {
  width: 44rpx;
  height: 70rpx;
  line-height: 70rpx;
  position: absolute;
  top: 0;
  color: #b9becd;
  font-size: 28rpx;
  text-align:center;
}

.week {
  left: 290rpx;
}

.total {
  left: 410rpx;
}

.weekOrTotal .hoverweekOrTotal {
  border-bottom: 1.8rpx solid #fff;
  color: #fff;
}

.Bg + view {
  height: 110rpx;
  background: url(https://vip-account.oss-cn-hangzhou.aliyuncs.com/mobile/static/global/images/li.png) no-repeat right center;
  background-size: contain;
  border-bottom: 2.8rpx solid #eee;
  line-height: 110rpx;
  font-size: 28rpx;
  color: #717481;
}

.matchTitle {
  height: 35rpx;
  line-height: 35rpx;
  border-left: 7rpx solid #a7aec0;
  float: left;
  margin-top: 40rpx;
  padding-left: 18rpx;
}

.myMatch {
  height: 110rpx;
  line-height: 110rpx;
  float: right;
  padding-right: 48rpx;
}

swiper {
  height: 540rpx;
}

swiper-item {
  height: 540rpx;
  width: 750rpx;
  position: relative;
}

.Rate {
  box-sizing: border-box;
  width: 220rpx;
  height: 220rpx;
  position: absolute;
  background: transparent;
  top: 42rpx;
  left: 271rpx;
}

.Rate > view {
  box-sizing: border-box;
  text-align: center;
}

.Rate > view:nth-of-type(1) {
  position: relative;
  top: 8rpx;
  left: 8rpx;
  width: 206rpx;
  height: 206rpx;
  background: #fff;  
  border-radius: 50%;
  z-index: 200;
}

.Rate > view:nth-of-type(2) {
  width: 110rpx;
  height: 220rpx;
  background: #fff; 
  position: absolute;
  top: 0;
  left: 0;
  border-radius: 220rpx 0 0 220rpx; 
  border: 14rpx #eee solid;
  z-index: 150;
}

.Rate >  view:nth-of-type(3) {
  width: 110rpx;
  height: 220rpx;
  background: #fff; 
  position: absolute;
  top: 0;
  right: 0;
  z-index: 100;
  border-radius: 0  220rpx 220rpx 0; 
  border: 14rpx #eee solid;
}

.Rate > view:nth-of-type(4) {
  width: 220rpx;
  height: 220rpx;
  border-radius: 50%;
  background: transparent;
  border: 14rpx #ee7142 solid;
  position: absolute;
  top: 0;
  left: 0;
  clip: rect(0, 110rpx, 220rpx, 0);
  z-index : 120;
}

.Rate > view:nth-of-type(5) {
  width: 220rpx;
  height: 220rpx;
  border-radius: 50%;
  background: transparent;
  border: 14rpx #ee7142 solid;
  position: absolute;
  top: 0;
  right: 0;
  clip: rect(0, 220rpx, 220rpx, 110rpx);
  z-index : 80;
}

.Rate view.RzIndex, .Rate view.RzIndex2{
  z-index: 160 !important;
}

.Rate .winRate {
  width: 100%;
  line-height: 76rpx;
  height: 76rpx;
  padding-top: 30rpx;
  font-family: 'Microsoft Yahei';
  font-size: 44rpx;
  color: #ee7142;
}

.winRate text {
  font-size: 24rpx;
  margin-left: -10rpx;
}

.winRate + view {
  width: 100%;
  line-height: 50rpx;
  height: 50rpx;
  font-size: 24rpx;
  color: #a7aec0;
}