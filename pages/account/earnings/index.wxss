page {
  background: #fff;
}
.common-header {
  background: transparent;
}

.Bg {
  width: 100%;
  height: 406rpx;
  background: url(https://vip-account.oss-cn-hangzhou.aliyuncs.com/mobile/static/account/urs/image/income.jpg) no-repeat center center;
  background-size: contain;
  margin-top: -1rpx;
  position: relative;
}

.userData {
  width: 100%;
  height: 120rpx;
  position: absolute;
  top: 124rpx;
  text-align: center;
}

.userData > view {
  width: 33.33%;
  float: left;
}

.userData > view:nth-child(1){
  color: #fff;
}

.userData > view:nth-child(2){
  color: #47a4ff;
}

.userData > view:nth-child(3){
  color: #e9bc38;
}

.userData > view view:nth-child(1){
  font-size: 42rpx;
  overflow: hidden;
  text-overflow: ellipsis;
}

.userData > view view:nth-child(2){
  font-size: 22rpx;
}

.weekOrTotal {
  width: 100%;
  height: 70rpx;
  border-top: 18rpx solid #eee;
  position: absolute;
  left: 0;
  top: 300rpx;
}

.weekOrTotal > view {
  width: 44rpx;
  height: 70rpx;
  line-height: 70rpx;
  position: absolute;
  top: 0;
  color: #b9becd;
  font-size: 28rpx;
  text-align:center;
}

.week {
  left: 290rpx;
}

.total {
  left: 410rpx;
}

.weekOrTotal .hoverweekOrTotal {
  border-bottom: 1.8rpx solid #fff;
  color: #fff;
}

.Bg + view {
  height: 110rpx;
  background: url(https://vip-account.oss-cn-hangzhou.aliyuncs.com/mobile/static/global/images/li.png) no-repeat right center;
  background-size: contain;
  border-bottom: 2.8rpx solid #eee;
  line-height: 110rpx;
  font-size: 28rpx;
  color: #717481;
}

.matchTitle {
  height: 35rpx;
  line-height: 35rpx;
  border-left: 7rpx solid #a7aec0;
  float: left;
  margin-top: 40rpx;
  padding-left: 18rpx;
}

.myMatch {
  height: 110rpx;
  line-height: 110rpx;
  float: right;
  padding-right: 48rpx;
}

swiper {
  height: 540rpx;
}

swiper-item {
  height: 540rpx;
  width: 750rpx;
  position: relative;
}

canvas {
  width: 216rpx;
  height: 216rpx;
  position: absolute;
  top: 42rpx;
  left: 271rpx;
}