.name {
  margin-top: 113rpx;
  margin-left: 3%;
  height: 112rpx;
  border: 1.4rpx solid rgb(153,153,153);
  border-radius: 10rpx;
}

.name input {
  box-sizing: border-box;
  width: 100%;
  height: 100%;
  font-size: 24rpx;
  padding-left: 32rpx;
  color: RGB(85,85,85);
  background: #fff;
  border-radius: 10rpx;
}

.limit {
  height: 50rpx;
  line-height: 50rpx;
  color: rgb(85,85,85);
  text-align: right;
  font-size: 22rpx;
}

.confirm {
  background: #e24f26;
  height: 100rpx;
  line-height: 100rpx;
  border-radius: 10.5rpx;
  font-size: 33.6rpx;
}