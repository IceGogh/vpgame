.msgList {
  margin-top: 84rpx;
}

.msgList > view {
  width: 96%;
  margin: 0 auto;
  border-bottom: 1.4rpx #dfdfdf solid;
  height: 126rpx;
  color: #858585;
  font-size: 24rpx;
}

.msgItem view:nth-of-type(1){
  margin-top: 16rpx;
  height: 104rpx;
  line-height: 52rpx;
  width: 70%;
  float: left;
  overflow: hidden;
  text-overflow: ellipsis;
}

.msgItem :nth-child(2){
  height: 104rpx;
  line-height: 126rpx;
  width: 22%;
  float: right;
}

