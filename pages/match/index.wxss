/*ball*/
.ball{
  position: relative;
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
}
.ball > view{
  width: 92rpx;
  height: 92rpx;
  border-radius: 50%;
  position: absolute;
  top: 0;
  left: 0;

}
.balla{
  border: 4rpx solid #e8e8e8;
  clip: rect(0,50rpx,100rpx,0);
}
.ballb{
  border: 4rpx solid #e8e8e8;
  clip: rect(0,100rpx,100rpx,50rpx);
}
.ballc{
  border: 4rpx solid #52ddbc;
  z-index: 5;
  transition: transform 1s cubic-bezier(0.075, 0.82, 0.165, 1);
}
.balld{
  border: 4rpx solid #52ddbc;
  z-index: 5;
  transition: transform 1s cubic-bezier(0.075, 0.82, 0.165, 1);
  -moz-transition-delay: 1s;
}
.ballBig .balla{
  z-index: 9;
}
.ballBig .ballb{
  z-index: 1;
}
.ballBig .ballc{
  clip: rect(0,50rpx,100rpx,0);
}

.ballBig .balld{
  clip: rect(0,50rpx,100rpx,0);
}
.ballSmall .balla{
  z-index: 1;
}
.ballSmall .ballb{
  z-index: 9;
}
.ballSmall .ballc{
  clip: rect(0,100rpx,100rpx,50rpx);
}
.ballSmall .balld{
  clip: rect(0,100rpx,100rpx,50rpx);
  opacity: 0;
}

.pankou-child{
  overflow: hidden; 
  transition: height .8s cubic-bezier(.5, .5, .5, .5);
  position: relative;
  
}
.pankou-child::before{
  content: ''; 
  position: absolute;
  width: 0rpx;
  height: 100%;
  left: 18rpx;
  top: 0;
  background-color: rgba(211, 211, 211, .6);
  box-shadow: .7rpx 0 2.1rpx 0 rgba(211, 211, 211, .6) ;
}
.pankou-child::after{
  content: '';
  position: absolute;
  width: 0rpx;
  height: 100%;
  left: 732rpx;
  top: 0;
  background-color: rgba(211, 211, 211, .6);
  box-shadow: .7rpx 0 2.1rpx 0 rgba(211, 211, 211, .6) ;
}
